{
  "manifest_version": 2,

  "name": "Sort Markdown List",
  "description": "Sorts markdown lists alphabetically.",
  "version": "1.2",

  "permissions": [ "contextMenus", "activeTab" ],
  "background": {
    "persistent": false,
    "scripts": [ "background.js" ]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
